  "license": "MIT",
  "dependencies": {
    "eventemitter2": "^6.4.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/borecjeborec1/lepikEvents.git"
  },
  "bugs": {
    "url": "https://github.com/borecjeborec1/lepikEvents/issues"
  },
  "homepage": "https://github.com/borecjeborec1/lepikEvents#readme"
